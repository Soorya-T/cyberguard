[run]
source = app
branch = True
omit =
    */tests/*
    */__pycache__/*
    */site-packages/*
    */venv/*
    */.venv/*
    */alembic/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
ignore_errors = True
skip_empty = True
precision = 2

[html]
directory = htmlcov

[xml]
output = coverage.xml

[json]
output = coverage.json
